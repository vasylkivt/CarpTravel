{
  "semi": true,
  "singleQuote": true,
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "useTabs": false,
  "proseWrap": "always",

  "plugins": ["prettier-plugin-tailwindcss"]
}
